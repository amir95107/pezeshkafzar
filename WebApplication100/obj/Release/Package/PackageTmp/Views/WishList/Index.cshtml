@{
    Layout = "~/Views/Shared/_DefaultLayout.cshtml";
}


<div id="wishList">
    @Html.Action("LikedProducts", "WishList")
</div>

<script>
    commandOrder = (id, count) => {
        $.ajax({
            url: "/ShopCart/CommandOrder/" + id,
            type: "Get",
            data: { count: count }
        }).done((res) => {
            $("#showOrder").html(res);
        });
    }
</script>